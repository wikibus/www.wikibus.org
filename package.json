{
  "name": "www.wikibus.org",
  "version": "0.1.12",
  "description": "wikibus.org public transport encyclopedia",
  "keywords": [
    "bus",
    "tram",
    "trolleybus"
  ],
  "author": "Tomasz Pluskiewicz",
  "license": "ISC",
  "scripts": {
    "lint:eslint": "eslint --ext .js,.html,.ts . --ignore-path .gitignore --quiet --ignore-pattern '/src/vendor/js/*' --ignore-pattern '/src/functions.js'",
    "format:eslint": "eslint --ext .js,.html,.ts . --fix --ignore-path .gitignore --quiet --ignore-pattern '/src/vendor/js/*' --ignore-pattern '/src/functions.js'",
    "lint:prettier": "prettier \"**/*.{ts,js}\" --check",
    "format:prettier": "prettier \"**/*.{ts,js}\" --write",
    "lint": "npm run lint:eslint && npm run lint:prettier",
    "format": "npm run format:eslint && npm run format:prettier",
    "test": "karma start --coverage",
    "test:watch": "karma start --auto-watch=true --single-run=false",
    "test:update-snapshots": "karma start --update-snapshots",
    "test:prune-snapshots": "karma start --prune-snapshots",
    "test:compatibility": "karma start --compatibility all --coverage",
    "test:compatibility:watch": "karma start --compatibility all --auto-watch=true --single-run=false",
    "test:bs": "karma start karma.bs.config.js --compatibility all --coverage",
    "prestart": "npm rebuild node-sass",
    "start": "webpack-dev-server --mode development",
    "build": "npm run build:style; webpack --mode production",
    "build:style": "node-sass src/style.scss > src/style.css",
    "release": "standard-version"
  },
  "dependencies": {
    "@auth0/auth0-spa-js": "^1.6.3",
    "@hydrofoil/alcaeus-forms": "^0.1.1",
    "@hydrofoil/alcaeus-loader": "^0.3.4",
    "@hydrofoil/hydrofoil-shell": "^0.3.2",
    "@lit-any/forms": "^0.12.3",
    "@lit-any/views": "^0.9.5",
    "@lit-element-bootstrap/button": "^1.0.0-alpha.7",
    "@lit-element-bootstrap/dropdown": "^1.0.0-alpha.4",
    "@lit-element-bootstrap/modal": "^1.0.0-alpha.7",
    "@polymer/iron-dropdown": "^3.0.1",
    "@polymer/paper-progress": "^3.0.1",
    "@rdfjs/namespace": "^1.1.0",
    "@rdfjs/parser-n3": "^1.1.4",
    "@tpluscode/rdf-ns-builders": "^0.1.0",
    "@vaadin/vaadin-context-menu": "^4.3.15",
    "@vaadin/vaadin-date-picker": "^4.0.8",
    "alcaeus": "^0.10.6",
    "feather-icon-literals": "^1.0.0-rc.8",
    "flyd": "^0.2.8",
    "iso-639-1": "^2.1.0",
    "ld-navigation": "^1.1.1",
    "lit-element": "^2.2.1",
    "lit-html": "^1.1.2",
    "meiosis-setup": "^2.0.0",
    "multiselect-combo-box": "^2.2.0",
    "once": "^1.4.0",
    "patchinko": "^4.2.0",
    "simple-icons": "^2.4.0"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-decorators": "^7.8.3",
    "@babel/plugin-proposal-optional-chaining": "^7.8.3",
    "@babel/preset-typescript": "^7.8.3",
    "@commitlint/cli": "^8.0.0",
    "@commitlint/config-conventional": "^8.3.4",
    "@open-wc/building-webpack": "^2.1.0",
    "@open-wc/eslint-config": "^2.0.3",
    "@open-wc/prettier-config": "^0.1.14",
    "@open-wc/testing": "^0.11.1",
    "@open-wc/testing-karma": "^3.2.42",
    "@open-wc/testing-karma-bs": "^1.3.40",
    "@tpluscode/eslint-config": "latest",
    "@types/jquery": "^3.3.31",
    "@types/once": "^1.4.0",
    "@types/mergerino": "^0.4.0",
    "@types/rdf-js": "^2.0.11",
    "@types/rdfjs__namespace": "^1.1.1",
    "@types/rdfjs__parser-n3": "^1.1.2",
    "@types/simple-icons": "^2.5.0",
    "@typescript-eslint/eslint-plugin": "^2.19.2",
    "@typescript-eslint/parser": "^2.6.0",
    "copy-webpack-plugin": "^5.1.1",
    "css-loader": "^3.4.2",
    "cz-conventional-changelog": "^3.1.0",
    "dotenv-webpack": "^1.7.0",
    "eslint-plugin-lit": "^1.2.0",
    "husky": "^4.2.1",
    "lint-staged": "^8.0.0",
    "node-sass": "^4.13.1",
    "raw-loader": "^4.0.0",
    "sass-loader": "^7.3.1",
    "standard": "^13.1.0",
    "standard-version": "^7.1.0",
    "style-loader": "^1.1.3",
    "typescript": "^3.7.2",
    "webpack": "^4.28.0",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.3",
    "webpack-merge": "^4.2.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && tsc --noEmit",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.{ts,js}": [
      "eslint --fix --quiet --ignore-path .gitignore --quiet --ignore-pattern '/src/vendor/js/*' --ignore-pattern '/src/functions.js'",
      "prettier --write",
      "git add"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  }
}
